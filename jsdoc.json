{
    "plugins": [""],
    
    "source": {
        "include": ["public/scripts/formUser"],
       
        "includePattern": ".js",
        "excludePattern": "(node_modules|docs)"
    },
    
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "encoding": "utf8",
        
        "recurse": true,
        "template": "node_modules/docdash",
    
        "destination":"./doc"
    }
}